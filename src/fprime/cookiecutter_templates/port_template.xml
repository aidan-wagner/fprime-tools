<?xml version='1.0' encoding='UTF-8'?>
<?xml-model href="{{path_to_fprime_root}}/../Autocoders/Python/schema/default/component_schema.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<interface name="{{port_name}}" namespace="{{namespace}}">
    <comment>
    {{short_description}}
    </comment>
    <args>
    {% for i in range(arg_number) %}
        <arg name="arg {{ i }}" type="U32">
            <comment>Argument {{ i }}</comment>
		</arg>
    {% endfor %}
    </args>
</interface>